    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "next-themes": "^0.4.6",
    "react-hook-form": "^7.53.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@scibo/eslint-config": "0.3.0",
    "@scibo/prettier-config": "0.1.0",
    "@scibo/tailwind-config": "0.1.0",
    "@scibo/tsconfig": "0.1.0",
    "@types/react": "19.1.12",
    "eslint": "9.12.0",
    "prettier": "3.3.3",
    "react": "19.1.1",
    "typescript": "5.6.3",
    "zod": "4.1.5"
  },
  "peerDependencies": {
    "react": "19.1.1",
    "zod": "4.1.5"
  },
  "prettier": "@scibo/prettier-config"
}
