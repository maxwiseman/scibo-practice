{
  "name": "@scibo/ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./*": {
      "default": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ui-add": "bunx shadcn@latest add && prettier src --write --list-different"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.4.3",
    "react-hook-form": "^7.53.2",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "usehooks-ts": "^3.1.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@scibo/eslint-config": "workspace:*",
    "@scibo/prettier-config": "workspace:*",
    "@scibo/tailwind-config": "workspace:*",
    "@scibo/tsconfig": "workspace:*",
    "@types/react": "^18.3.11",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "react": "18.3.1",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "zod": "^3.23.8"
  },
  "prettier": "@scibo/prettier-config"
}
