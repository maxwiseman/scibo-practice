    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "prettier": "@scibo/prettier-config",
  "dependencies": {
    "@scibo/db": "workspace:*",
    "pdf.js-extract": "^0.2.1",
    "puppeteer-core": "^23.10.4"
  }
}
