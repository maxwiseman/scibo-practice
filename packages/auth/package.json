    "next": "^14.2.15",
    "next-auth": "5.0.0-beta.20",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@scibo/eslint-config": "0.3.0",
    "@scibo/prettier-config": "0.1.0",
    "@scibo/tsconfig": "0.1.0",
    "eslint": "9.12.0",
