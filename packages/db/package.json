{
  "name": "@scibo/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./src/schema.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "bun run with-env drizzle-kit push",
    "studio": "bun run with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "drizzle-orm": "^0.35.1",
    "drizzle-zod": "^0.5.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@scibo/eslint-config": "0.3.0",
    "@scibo/prettier-config": "0.1.0",
    "@scibo/tsconfig": "0.1.0",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "9.12.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3"
  },
  "prettier": "@scibo/prettier-config"
}
